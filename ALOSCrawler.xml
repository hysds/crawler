<?xml version="1.0"?>
<crawlerConfig xmlns="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:sf="http://sciflo.jpl.nasa.gov/2006v1/sf"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <instrument>ALOS</instrument>
    <level>L2</level>
    <dataLocations>
        <location>
            <protocol>LOCAL</protocol>
            <site>localhost</site>
            <rootDir>/data/public/repository/frame_alos</rootDir>
            <matchFileRegEx>(ALPSRP\d{9})</matchFileRegEx>
            <objectidTemplate>($1)</objectidTemplate>
        </location>
    </dataLocations>
</crawlerConfig>
